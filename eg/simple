#!/usr/bin/env perl
use strict;
use warnings;
use FindBin;
use lib "$FindBin::Bin/../lib/";
use Docopt;
use Data::Dumper;
use JSON;

our $VERSION = '0.02';

my $ret = docopt(version => $VERSION);
print JSON->new->canonical(1)->convert_blessed->encode($ret), "\n";

__END__

=head1 NAME

simple - very simple example

=head1 SYNOPSIS

    simple [--foo]

=head1 DESCRIPTION

Hah!
